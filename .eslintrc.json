{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["jquery"],
  "env": {
    "jquery": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "quotes": [2, "single"],
    "eqeqeq": 2,
    "prefer-const": 2
  }
}
